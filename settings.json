{
  "files.autoSave": "afterDelay",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "code-runner.runInTerminal": true,
  "cmake.defaultVariants": {
    "buildType": {
      "default": "debug",
      "description": "The build type.",
      "choices": {
        "debug": {
          "short": "Debug",
          "long": "Disable optimizations - include debug information.",
          "buildType": "Debug",
        },
        "release": {
          "short": "Release",
          "long": "Optimize for speed - exclude debug information.",
          "buildType": "Release",
        },
        "minsize": {
          "short": "MinSizeRel",
          "long": "Optimize for smallest binary size - exclude debug information.",
          "buildType": "MinSizeRel",
        },
        "reldeb": {
          "short": "RelWithDebInfo",
          "long": "Optimize for speed - include debug information.",
          "buildType": "RelWithDebInfo",
        },
        "unspecified": {
          "short": "Unspecified",
          "long": "Let CMake pick the default build type.",
          "buildType": "Unspecified",
        },
      },
    },
  },
  "C_Cpp.default.compilerPath": "/opt/homebrew/bin/g++-15",
  "workbench.settings.applyToAllProfiles": [],
  "editor.fontSize": 14,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "cpp": false,
    "javascript": true,
    "typescript": true,
    "jsonc": true,
    "json": true,
    "typescriptreact": true,
    "css": true,
  },
  "editor.stickyScroll.enabled": false,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#d064ff",
    "editorCursor.background": "#000000",
    "terminal.background": "#00000000",
    "terminalCursor.foreground": "#dc6efa",
  },
  "window.titleBarStyle": "native",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "background.fullscreen": {
    "images": ["file:///Users/adarsh/Downloads/wp/6.jpg"],

    "opacity": 0.09,
    "size": "contain",
    "position": "center",
    "interval": 0,
    "random": false,
  },
  "background.enabled": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "git.autofetch": true,
  "workbench.sideBar.location": "right",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "explorer.compactFolders": false,
  "editor.cursorBlinking": "solid",
  "editor.renderLineHighlight": "none",

  "vscode_custom_css.imports": [
    "file:///Users/adarsh/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js",
    "file:///Users/adarsh/cursor.js",
    "file:////Users/adarsh/Developer/mydotfiles/custom.css",
  ],
  "workbench.tree.enableStickyScroll": false,
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": true,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "repositories": false,
    "searchAndCompare": false,
    "launchpad": true,
  },
  "workbench.activityBar.location": "top",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "editor.lineHeight": 2.0,

  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
  ],
  "excalidraw.theme": "dark",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "workbench.editor.tabSizingFixedMaxWidth": 150,
  "editor.renderWhitespace": "none",
  "debug.disassemblyView.showSourceCode": false,
  "animations.Enabled": true,
  "cph.language.cpp.Command": "g++-15",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "markdown-preview-enhanced.previewTheme": "atom-dark.css",
  "markdown-preview-enhanced.enablePreviewZenMode": false,
  "editor.fontLigatures": true,

  "npm.enableRunFromFolder": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.instructionsFilesLocations": {
    ".github/instructions": true,
    ".claude/rules": true,
    "/var/folders/fn/7_ctbd8n111br2rt1hc0btkh0000gn/T/postman-collections-post-response.instructions.md": true,
    "/var/folders/fn/7_ctbd8n111br2rt1hc0btkh0000gn/T/postman-collections-pre-request.instructions.md": true,
    "/var/folders/fn/7_ctbd8n111br2rt1hc0btkh0000gn/T/postman-folder-post-response.instructions.md": true,
    "/var/folders/fn/7_ctbd8n111br2rt1hc0btkh0000gn/T/postman-folder-pre-request.instructions.md": true,
    "/var/folders/fn/7_ctbd8n111br2rt1hc0btkh0000gn/T/postman-http-request-post-response.instructions.md": true,
    "/var/folders/fn/7_ctbd8n111br2rt1hc0btkh0000gn/T/postman-http-request-pre-request.instructions.md": true
  },
  "editor.inlineSuggest.edits.showCollapsed": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "C_Cpp.default.intelliSenseMode": "macos-gcc-arm64",
  "C_Cpp.default.includePath": [
    "/opt/homebrew/include",
    "/opt/homebrew/include/c++/15",
  ],
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.colorTheme": "Tokyo Night Pro",
  "cph.general.defaultLanguage": "none",
  "cph.language.c.Command": "gcc ",
  "cph.general.showLiveUserCount": true,
  "cph.language.cpp.Args": "-DDEBUG",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format",
  },
  "workbench.editor.showTabs": "none",
  "editor.formatOnSave": true,
  "git.enableSmartCommit": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "storage.modifier",
          "storage.type",
          "support.class.builtin",
          "keyword.control",
          "constant.language",
          "entity.other.attribute-name",
          "entity.name.method",
        ],
        "settings": {
          "fontStyle": "italic",
        },
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
        ],
        "settings": {
          "fontStyle": "",
        },
      },
    ],
  },

  "editor.fontFamily": "Fira Code,Menlo, Monaco, 'Courier New', monospace",
  "editor.fontWeight": "500",
  "editor.minimap.enabled": false,
  "workbench.editor.editorActionsLocation": "titleBar",
  "editor.scrollbar.verticalScrollbarSize": 6,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "workbench.iconTheme": "flow-dawn",
  "json.schemas": [],
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "zig": "zig run",
    "cpp": "cd $dir && g++-15 -std=c++23 -DDEBUG  $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "bun",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runghc",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "sml": "cd $dir && sml $fileName",
    "mojo": "mojo run",
    "erlang": "escript",
    "spwn": "spwn build",
    "pkl": "cd $dir && pkl eval -f yaml $fileName -o $fileNameWithoutExt.yaml",
    "gleam": "gleam run -m $fileNameWithoutExt",
  },
  "postman.mcp.notifications.postmanMCP": false,
  "C_Cpp.default.cppStandard": "c++23",
  "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.stickyScroll.enabled": false,
  "[java]": {
    "editor.defaultFormatter": "xaver.clang-format",
  },
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
  },
  "window.customTitleBarVisibility": "never",
  "github.copilot.selectedCompletionModel": "",
  "breadcrumbs.enabled": false,
  "editor.scrollbar.vertical": "auto",
}
